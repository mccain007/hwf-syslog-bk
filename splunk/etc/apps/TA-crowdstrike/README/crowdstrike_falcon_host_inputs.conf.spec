[<input_name>]
account      = account name in crowdstrike_falcon_host_accounts.conf
app_id       = <optional> APP ID to connect the falcon host (maximun length is 10)
start_offset = <optional> the start offset from where the events should be seeked
start_date   = <optional> the start date from when the events should be seeked
interval     = task invocation interval
index        = index of the fetched data
