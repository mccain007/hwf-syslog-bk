{
    "meta": {
        "apiVersion": "3.0.0", 
        "name": "TA-crowdstrike_falcon_intel", 
        "displayName": "CrowdStrike Falcon Intelligence Add-on", 
        "restRoot": "TA_crowdstrike_falcon_intel", 
        "version": "1.0.0"
    }, 
    "pages": {
        "inputs": {
            "services": [
                {
                    "name": "falcon_intelligence_data_input", 
                    "entity": [
                        {
                            "validators": [
                                {
                                    "pattern": "^[a-zA-Z]\\w*$", 
                                    "type": "regex", 
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores."
                                }, 
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of input name should be between 1 and 100", 
                                    "type": "string", 
                                    "maxLength": 100
                                }
                            ], 
                            "type": "text", 
                            "help": "Enter a unique name for the data input", 
                            "label": "Name", 
                            "required": true, 
                            "field": "name"
                        }, 
                        {
                            "validators": [
                                {
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$", 
                                    "type": "regex", 
                                    "errorMsg": "Interval must be an integer."
                                }
                            ], 
                            "type": "text", 
                            "help": "Time interval of input in seconds.", 
                            "label": "Interval", 
                            "required": true, 
                            "field": "interval"
                        }, 
                        {
                            "validators": [
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of index name should be between 1 and 80.", 
                                    "type": "string", 
                                    "maxLength": 80
                                }
                            ], 
                            "defaultValue": "default", 
                            "options": {
                                "blackList": "^_.*$", 
                                "endpointUrl": "data/indexes", 
                                "createSearchChoice": true
                            }, 
                            "type": "singleSelect", 
                            "label": "Index", 
                            "required": true, 
                            "field": "index"
                        }, 
                        {
                            "options": {
                                "referenceName": "account"
                            }, 
                            "type": "singleSelect", 
                            "help": "", 
                            "label": "Falcon Intelligence Account", 
                            "field": "falcon_intelligence_account", 
                            "required": true
                        }, 
                        {
                            "defaultValue": "All", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ], 
                            "type": "text", 
                            "help": "Date (UTC in \"YYYY-MM-DD hh:mm:ss\" format) from when to start collecting the data. For default value (2000-01-01 00:00:00 UTC.) type All.", 
                            "label": "Starting Date", 
                            "field": "starting_date", 
                            "required": false
                        }
                    ], 
                    "title": "Falcon Intelligence Data Input"
                }
            ], 
            "title": "Inputs", 
            "description": "Manage your data inputs", 
            "table": {
                "moreInfo": [
                    {
                        "field": "name", 
                        "label": "Name"
                    }, 
                    {
                        "field": "interval", 
                        "label": "Interval"
                    }, 
                    {
                        "field": "index", 
                        "label": "Index"
                    }, 
                    {
                        "field": "disabled", 
                        "label": "Status"
                    }, 
                    {
                        "field": "falcon_intelligence_account", 
                        "label": "Falcon Intelligence Account"
                    }, 
                    {
                        "field": "starting_date", 
                        "label": "Starting Date"
                    }
                ], 
                "header": [
                    {
                        "field": "name", 
                        "label": "Name"
                    }, 
                    {
                        "field": "interval", 
                        "label": "Interval"
                    }, 
                    {
                        "field": "index", 
                        "label": "Index"
                    }, 
                    {
                        "field": "disabled", 
                        "label": "Status"
                    }
                ], 
                "actions": [
                    "edit", 
                    "enable", 
                    "delete", 
                    "clone"
                ]
            }
        }, 
        "configuration": {
            "title": "Configuration", 
            "description": "Configure your Falcon Intelligence Account, Proxy and logging level.", 
            "tabs": [
                {
                    "entity": [
                        {
                            "validators": [
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of Account name should be between 1 and 50", 
                                    "type": "string", 
                                    "maxLength": 50
                                }, 
                                {
                                    "pattern": "^[a-zA-Z]\\w*$", 
                                    "type": "regex", 
                                    "errorMsg": "Account name must start with a letter and followed by alphabetic letters, digits or underscores."
                                }
                            ], 
                            "type": "text", 
                            "help": "Enter a unique name for this account.", 
                            "label": "Account Name", 
                            "required": true, 
                            "field": "name"
                        }, 
                        {
                            "validators": [
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of username should be between 1 and 200", 
                                    "type": "string", 
                                    "maxLength": 200
                                }
                            ], 
                            "options": {
                                "placeholder": "Enter the username here"
                            }, 
                            "type": "text", 
                            "help": "Enter the username for this account.", 
                            "label": "Username", 
                            "required": true, 
                            "field": "username"
                        }, 
                        {
                            "field": "password", 
                            "validators": [
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of password should be between 1 and 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ], 
                            "type": "text", 
                            "help": "Enter the password for this account.", 
                            "label": "Password", 
                            "encrypted": true, 
                            "required": true
                        }
                    ], 
                    "name": "account", 
                    "table": {
                        "header": [
                            {
                                "field": "name", 
                                "label": "Account Name"
                            }, 
                            {
                                "field": "username", 
                                "label": "Username"
                            }
                        ], 
                        "actions": [
                            "edit", 
                            "delete", 
                            "clone"
                        ]
                    }, 
                    "title": "Account"
                }, 
                {
                    "name": "proxy", 
                    "options": {
                        "saveValidator": "function(formData) { if(!formData.proxy_enabled || formData.proxy_enabled === '0') {return true; } if(!formData.proxy_url) { return 'Proxy Host can not be empty'; } if(!formData.proxy_port) { return 'Proxy Port can not be empty'; } return true; }"
                    }, 
                    "entity": [
                        {
                            "field": "proxy_enabled", 
                            "type": "checkbox", 
                            "label": "Enable"
                        }, 
                        {
                            "defaultValue": "http", 
                            "options": {
                                "disableSearch": true, 
                                "autoCompleteFields": [
                                    {
                                        "value": "http", 
                                        "label": "http"
                                    }, 
                                    {
                                        "value": "socks4", 
                                        "label": "socks4"
                                    }, 
                                    {
                                        "value": "socks5", 
                                        "label": "socks5"
                                    }
                                ]
                            }, 
                            "field": "proxy_type", 
                            "type": "singleSelect", 
                            "label": "Proxy Type"
                        }, 
                        {
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max host length is 4096", 
                                    "type": "string", 
                                    "maxLength": 4096
                                }
                            ], 
                            "field": "proxy_url", 
                            "type": "text", 
                            "label": "Host"
                        }, 
                        {
                            "validators": [
                                {
                                    "range": [
                                        1, 
                                        65535
                                    ], 
                                    "type": "number"
                                }
                            ], 
                            "field": "proxy_port", 
                            "type": "text", 
                            "label": "Port"
                        }, 
                        {
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of username is 50", 
                                    "type": "string", 
                                    "maxLength": 50
                                }
                            ], 
                            "field": "proxy_username", 
                            "type": "text", 
                            "label": "Username"
                        }, 
                        {
                            "encrypted": true, 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of password is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ], 
                            "field": "proxy_password", 
                            "type": "text", 
                            "label": "Password"
                        }, 
                        {
                            "field": "proxy_rdns", 
                            "type": "checkbox", 
                            "label": "Remote DNS resolution"
                        }
                    ], 
                    "title": "Proxy"
                }, 
                {
                    "name": "logging", 
                    "entity": [
                        {
                            "defaultValue": "INFO", 
                            "options": {
                                "disableSearch": true, 
                                "autoCompleteFields": [
                                    {
                                        "value": "DEBUG", 
                                        "label": "DEBUG"
                                    }, 
                                    {
                                        "value": "INFO", 
                                        "label": "INFO"
                                    }, 
                                    {
                                        "value": "WARNING", 
                                        "label": "WARNING"
                                    }, 
                                    {
                                        "value": "ERROR", 
                                        "label": "ERROR"
                                    }, 
                                    {
                                        "value": "CRITICAL", 
                                        "label": "CRITICAL"
                                    }
                                ]
                            }, 
                            "field": "loglevel", 
                            "type": "singleSelect", 
                            "label": "Log level"
                        }
                    ], 
                    "title": "Logging"
                }
            ]
        }
    }
}